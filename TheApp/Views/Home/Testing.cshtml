@{
    ViewData["Title"] = "Testing Items";
}

<form asp-action="Upload" asp-controller="Home" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="file">Choose a file:</label>
        <input type="file" name="file" id="file" class="form-control" required>
    </div>
    <button type="submit" class="btn btn-primary">Upload</button>
</form>


@await Component.InvokeAsync("Post")

<partial name="/Views/File/_filePartial.cshtml" />

@Html.Partial("/Views/Partial/_testPartial.cshtml")
    
 

